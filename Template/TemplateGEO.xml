<?xml version="1.0" encoding="UTF-8"?>
<GPM profile="GEO">
    <FGDC_Required>
        <ISO_Theme>
            <Theme_Keyword_Theasuruas/>
            <!-- Add as many Theme_Keyword elements as needed -->
            <Theme_Keyword>Boundaries</Theme_Keyword>
        </ISO_Theme>
        <NGDA_Info>
            <!-- optional used only for those datasets tagged as NGDA -->
            <NGDA_Keyword_Thesaurus/>
            <!-- add as many NGDA_Keywords as needed -->
            <NGDA_Keyword>NGDA</NGDA_Keyword>
            <GETMAP_URL/>
            <REST_URL/>
        </NGDA_Info>
    </FGDC_Required>
    <Identification_Information>
        <Citation>
            <Originator/>
            <Publication_Date/>
            <Title/>
            <Alternate_Title><!-- optional used only for those datasets tagged as NGDA --></Alternate_Title>
            <Edition/>
            <Geospatial_Data_Presentation_Form/>
            <Series_Information>
                <!-- optional -->
                <Series_Name/>
                <Issue_Identification/>
            </Series_Information>
            <Online_Linkage/>
        </Citation>
        <Description>
            <Abstract/>
            <Purpose/>
        </Description>
        <Time_Period_of_Content>
            <!-- choose either Calendar_Date or Range_Of_Dates 
            <Calendar_Date/>-->
            <Range_Of_Dates>
                <Beginning_Date/>
                <Ending_Date/>
            </Range_Of_Dates>
            <Currentness_Reference/>
        </Time_Period_of_Content>
        <Status>
            <Progress/>
            <Maintenance_and_Update_Frequency/>
        </Status>
        <Spatial_Domain>
            <!-- Choose either Bounding_Coordinates or Data_Set_G-Polygon -->
            <Bounding_Coordinates>
                <West_Bounding_Coordinate/>
                <East_Bounding_Coordinate/>
                <North_Bounding_Coordinate/>
                <South_Bounding_Coordinate/>
            </Bounding_Coordinates>
        </Spatial_Domain>
        <Keywords>
            <Theme>
                <!-- Use as many Theme elements as needed. Use one for each Thesaurus -->
                <Theme_Keyword_Thesaurus/>
                <!-- Add as many Theme_Keyword elements as needed -->
                <Theme_Keyword/>
            </Theme>
            <Place>
                <!-- Use A Place element for every level of Geography (Nation, State, County) -->
                <Place_Keyword_Thesaurus><!-- Use one of the approved ANSI codes -->
                </Place_Keyword_Thesaurus>
                <!-- Add as many Place_Keyword elements as needed  -->
                <Place_Keyword>United States</Place_Keyword>
            </Place>
        </Keywords>
        <Constraints>
            <Access_Constraints/>
            <Use_Constraints/>
            <Other_Constraints>
                <!-- optional --></Other_Constraints>
        </Constraints>
        <Point_of_Contact>
            <Contact_Organization/>
            <Contact_Address>
                <Address/>
                <City/>
                <State_or_Province/>
                <Postal_Code/>
                <Country/>
            </Contact_Address>
            <Contact_Voice_Telephone/>
            <Contact_Electronic_Mail_Address/>
        </Point_of_Contact>
        <Browse_Graphic>
            <!-- optional -->
            <Browse_Graphic_File_Name/>
            <Browse_Graphic_File_Description/>
            <Browse_Graphic_File_Type/>
        </Browse_Graphic>
        <Data_Set_Character_Set>UTF-8</Data_Set_Character_Set>
        <Data_Set_Language>eng</Data_Set_Language>
        <Cross_Reference>
            <!-- This is optional -->
            <Citation>
                <Originator/>
                <Publication_Date/>
                <Title/>
                <Online_Linkage/>
            </Citation>
        </Cross_Reference>
    </Identification_Information>
    <Distribution_Information>
        <Point_of_Contact>
            <Contact_Organization/>
            <Contact_Address>
                <Address/>
                <City/>
                <State_or_Province/>
                <Postal_Code/>
                <Country/>
            </Contact_Address>
            <Contact_Voice_Telephone/>
            <Contact_Electronic_Mail_Address/>
        </Point_of_Contact>
        <Distribution_Liability/>
        <Standard_Order_Process>
            <!-- repeat as many times as needed -->
            <Digital_Form>
                <!-- repeat as many times as needed -->
                <Format_Name/>
                <!-- choose either Format_Version_Date or Format_Version_Number -->
                <Format_Version_Date/>
                <!--   <Format_Version_Number></Format_Version_Number>-->
                <File_Decompression_Technique/>
                <Transfer_Size><!-- optional --></Transfer_Size>
                <Network_Address>
                    <Network_Resource_Name><!-- repeat as many times as needed --></Network_Resource_Name>
                    <mimeType>application/x-zip-compressed</mimeType>
                </Network_Address>
            </Digital_Form>
            <Fees/>
            <Ordering_Instructions><!-- optional --></Ordering_Instructions>
        </Standard_Order_Process>
        <Technical_Prerequisites/>
    </Distribution_Information>
    <Metadata_Reference_Information>
        <Metadata_Date/>
        <Metadata_Parent_Identifier/>
        <Metadata_Hierarchy_Level>dataset</Metadata_Hierarchy_Level>
        <Metadata_Hierarchy_Level_Name/>
        <Point_of_Contact>
            <Contact_Organization/>
            <Contact_Address>
                <Address/>
                <City/>
                <State_or_Province/>
                <Postal_Code/>
                <Country/>
            </Contact_Address>
            <Contact_Voice_Telephone/>
            <Contact_Electronic_Mail_Address/>
        </Point_of_Contact>
        <Metadata_Standard_Name/>
        <Metadata_Standard_Version/>
        <Metadata_Character_Set>UTF-8</Metadata_Character_Set>
        <Metadata_File_Identifier/>
        <Metadata_Language>eng</Metadata_Language>
    </Metadata_Reference_Information>
</GPM>
