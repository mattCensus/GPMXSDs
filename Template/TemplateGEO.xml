<?xml version="1.0" encoding="UTF-8"?>
<GPM profile="gdb">
    
    <!-- See Fill in here for all the element that must be filled in These elements have values, but should be changed by you -->
    <FGDC_Required>
        <ISO_Theme>
            <ISO_Keyword_Thesaurus>ISO 19115 Topic Categories</ISO_Keyword_Thesaurus>
            <ISO_Keyword>location</ISO_Keyword>
        </ISO_Theme>
    </FGDC_Required>
    <Identification_Information>
        <Citation>
            <Originator/>
            <Publication_Date>10142020</Publication_Date><!-- Fill in here Enter the date here -->
            <Title>Put the title here</Title>
           
            <Edition/>
            
            <Geospatial_Data_Presentation_Form>Geodatabase</Geospatial_Data_Presentation_Form>
            <Series_Information>
                <!-- optional -->
                <Series_Name/>
                <Issue_Identification/>
            </Series_Information>
            <Online_Linkage/>
        </Citation>
        <Description>
            <Abstract>Insert the abstract here</Abstract>
            <Purpose>Enter the purpose here</Purpose>
        </Description>
        <Time_Period_of_Content>
            <!-- choose either Calendar_Date or Range_of_Dates -->
           
            <Range_of_Dates>
                <Beginning_Date>20190101</Beginning_Date><!-- Fill in here -->
                <Ending_Date>20191231</Ending_Date> <!-- Fill in here -->
            </Range_of_Dates>
            <Currentness_Reference>ground condition</Currentness_Reference><!--Fill in here enter the curentness reference here -->
        </Time_Period_of_Content>
        <Status>
            <Progress>Completed</Progress> <!-- Enter the progress -->
            <Maintenance_and_Update_Frequency>notPlanned</Maintenance_and_Update_Frequency>
        </Status>
        <Spatial_Domain>
            <!-- Choose either Bounding_Coordinates or Data_Set_G-Polygon -->
            <Bounding_Coordinates>
                <West_Bounding_Coordinate>-179.231086</West_Bounding_Coordinate><!-- Fill in here -->
                <East_Bounding_Coordinate>179.859681</East_Bounding_Coordinate><!-- Fill in here -->
                <North_Bounding_Coordinate>71.441059</North_Bounding_Coordinate><!-- Fill in here -->
                <South_Bounding_Coordinate>-14.601813</South_Bounding_Coordinate><!-- Fill in here -->
            </Bounding_Coordinates>
        </Spatial_Domain>
        <Keywords>
            <Theme>
                <!-- Use as many Theme elements as needed. Use one for each Thesaurus -->
                <Theme_Keyword_Thesaurus>Enter the keyword</Theme_Keyword_Thesaurus>
                <!-- Add as many Theme_Keyword elements as needed -->
                <Theme_Keyword>Enter a keyword here</Theme_Keyword>
            </Theme>
            <Place>
                <!-- Use A Place element for every level of Geography (Nation, State, County) -->
                <Place_Keyword_Thesaurus><!-- Use one of the approved ANSI codes -->
                </Place_Keyword_Thesaurus>
                <!-- Add as many Place_Keyword elements as needed  -->
                <Place_Keyword>United States</Place_Keyword>
            </Place>
        </Keywords>
        <Constraints>
            <Access_Constraints>unrestricted</Access_Constraints><!-- Fill in here -->
            <Use_Constraints>trademark</Use_Constraints><!-- Fill in here -->
            <Other_Constraints>
                <!-- optional --></Other_Constraints>
        </Constraints>
        <Point_of_Contact>
            <Contact_Organization/>
            <Contact_Address>
                <Address/>
                <City/>
                <State_or_Province/>
                <Postal_Code/>
                <Country/>
            </Contact_Address>
            <Contact_Voice_Telephone/>
            <Contact_Electronic_Mail_Address/>
        </Point_of_Contact>
        <Browse_Graphic>
            <!-- optional -->
            <Browse_Graphic_File_Name/>
            <Browse_Graphic_File_Description/>
            <Browse_Graphic_File_Type/>
        </Browse_Graphic>
        <Data_Set_Character_Set>UTF-8</Data_Set_Character_Set>
        <Data_Set_Language>eng</Data_Set_Language>
        <Cross_Reference>
            <!-- This is optional -->
            <Citation>
                <Originator/>
                <Publication_Date>20201014</Publication_Date><!-- Enter a Publication date here -->
                <Title>Enter the title of the source here</Title><!-- Fill in here -->
                <Online_Linkage/>
            </Citation>
        </Cross_Reference>
    </Identification_Information>
    <Distribution_Information>
        <Point_of_Contact>
            <Contact_Organization/>
            <Contact_Address>
                <Address/>
                <City/>
                <State_or_Province/>
                <Postal_Code/>
                <Country/>
            </Contact_Address>
            <Contact_Voice_Telephone/>
            <Contact_Electronic_Mail_Address/>
        </Point_of_Contact>
        <Distribution_Liability/>
        <Standard_Order_Process>
            <!-- repeat as many times as needed -->
            <Digital_Form>
                <!-- repeat as many times as needed -->
                <Format_Name/>
                <!-- choose either Format_Version_Date or Format_Version_Number -->
                <Format_Version_Date>20201014</Format_Version_Date>
                 
                <File_Decompression_Technique/>
                <Transfer_Size><!-- optional --></Transfer_Size>
                <Network_Address>
                    <Network_Resource_Name><!-- repeat as many times as needed --></Network_Resource_Name>
                    <Mime_Type>application/zip</Mime_Type><!-- Fill in here -->
                </Network_Address>
            </Digital_Form>
            <Fees/>
            <Ordering_Instructions><!-- optional --></Ordering_Instructions>
        </Standard_Order_Process>
        <Technical_Prerequisites/>
    </Distribution_Information>
    <Metadata_Reference_Information>
        <Metadata_Date>20201014</Metadata_Date><!-- Fill in here -->
        <Metadata_Parent_Identifier/>
        <Metadata_Hierarchy_Level>dataset</Metadata_Hierarchy_Level><!-- Fill in here -->
        <Metadata_Hierarchy_Level_Name/>
        <Point_of_Contact>
            <Contact_Organization/>
            <Contact_Address>
                <Address/>
                <City/>
                <State_or_Province/>
                <Postal_Code/>
                <Country/>
            </Contact_Address>
            <Contact_Voice_Telephone/>
            <Contact_Electronic_Mail_Address/>
        </Point_of_Contact>
        <Metadata_Standard_Name/>
        <Metadata_Standard_Version/>
        <Metadata_Character_Set>UTF-8</Metadata_Character_Set>
        <Metadata_File_Identifier>Filename.gpm.xml</Metadata_File_Identifier><!-- Fill in here -->
        <Metadata_Language>eng</Metadata_Language>
    </Metadata_Reference_Information>
</GPM>
